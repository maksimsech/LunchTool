@model IEnumerable<LunchTool.Web.ViewModels.UserMonthReportViewModel>

<table class="table" id="reportTable">
    <thead class="thead-light">
        <tr>
            <th scope="col">День</th>
            <th scope="col">Количество заказов</th>
            <th scope="col">Стоймость</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td scope="row">@item.Day</td>
                <td>@item.OrderCount</td>
                <td>@item.Price</td>
            </tr>
        }
    </tbody>
</table>