@model IEnumerable<LunchTool.Web.ViewModels.UserMonthReportViewModel>

<table class="table">
    <tr>
        <td>День</td>
        <td>Количество заказов</td>
        <td>Стоймость</td>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Day</td>
            <td>@item.OrderCount</td>
            <td>@item.Price</td>
        </tr>
    }
</table>