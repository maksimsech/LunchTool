@model IEnumerable<LunchTool.Web.ViewModels.UserProvidersReportViewModel>

@foreach (var item in Model)
{
    <div class="text-right">@item.ProviderName</div>
    <table class="table">
        <tr>
            <td>
                ФИО
            </td>
            <td>
                Заказов
            </td>
            <td>
                Стоймость
            </td>
        </tr>
        <tr>
            <td>
                @item.UserName
            </td>
            <td>
                @item.OrderCount
            </td>
            <td>
                @item.Price
            </td>
        </tr>
    </table>
}